#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>

int main() {
  printf("Hello world!\n");
  char *buff = NULL;
  size_t size = 0;
  ssize_t num_char = getline(&buff, &size, stdin);
  printf("You entered: %s", buff);
  printf("numchar= %zd" , num_char);

  free(buff);
  return 0;
}
